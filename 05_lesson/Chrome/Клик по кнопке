from selenium import webdriver
from time import sleep

browser = webdriver.Crome()
browser.get("http://the-internet.herokuapp.com/add_remove_elements/")

for i in range(5):
    add_btn = browser.find_element_by_css_selektor("button[onclick^='addElement()']")
    add_btn.click()

delete_buttons = browser.find_element_by_css_selektor("button[onclick^='deleteElement()']")

print("Размер списка кнопок Delete:", len(delete_buttons))
